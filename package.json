  },
  "homepage": "https://github.com/EmmaEm/Nomad#readme",
  "dependencies": {
    "body-parser": "^1.18.1",
    "connect-pg-simple": "^4.2.1",
    "ejs": "^2.5.7",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "pg-promise": "^6.5.2"
  },
  "devDependencies": {
    "nodemon": "^1.12.0"
  }
