{
  "name": "nomad",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "setup": "src/config/setup.sh",
    "db:reset": "src/config/reset-db.sh",
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "db:create": "createdb nomad",
    "db:drop": "dropdb nomad",
    "db:schema": "psql -d nomad < src/db/schema.sql",
    "db:seed": "psql -d nomad < src/db/seed-data.sql",
    "load-session-store": "psql nomad < node_modules/connect-pg-simple/table.sql"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmmaEm/Nomad.git"
  },
