<title>Vinyl: User Name<%= //reviewsAndUser[0].name %>'s Profile</title>
<%- include partials/header.ejs %>

<div class="row-sp-btwn">

  <div class="one-third">
    <img src="#<%= //reviewsAndUser[0].photo %>" alt="User's profile photo">
  </div>

  <div class="two-thirds">
    <h1>Name<%= //reviewsAndUser[0].name %></h1>
    <div class="row-sp-btwn">
      <h3>Email</h3>
      <h3>Email <%= //reviewsAndUser[0].email %></h3>
    </div>
    <div class="row-sp-btwn">
      <h3>Date Joined</h3>
      <h3>Date<%= //reviewsAndUser[0].join_date %></h3>
    </div>

    <%- include partials/reviews.ejs %>

  </div>


</div>

<%- include partials/footer.ejs %>


